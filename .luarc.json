{
  "$schema": "https://raw.githubusercontent.com/LuaLS/vscode-lua/master/setting/schema.json",
  "diagnostics.unusedLocalExclude": ["_*"],
  "workspace.ignoreDir": [".vscode"],
  "workspace": {
    "library": ["/usr/share/awesome/lib/", "./deps"]
  },
  "runtime.path": [
    "/usr/local/share/lua/5.3/?.lua",
    "/usr/local/share/lua/5.3/?/init.lua",
    "/usr/local/lib/lua/5.3/?.lua",
    "/usr/local/lib/lua/5.3/?/init.lua",
    "/usr/share/lua/5.3/?.lua",
    "/usr/share/lua/5.3/?/init.lua",
    "./deps/lua-stream/?.lua",
    "./deps/lua-stream/?/init.lua",
    "./deps/?.lua",
    "./deps/.build/lib/lua/5.4/posix/?.lua",
    "./deps/.build/lib/lua/5.4/posix/?/init.lua",
    "./deps/?/init.lua",
    "./?.lua",
    "./?/init.lua",
    "/etc/xdg/awesome/?.lua",
    "/etc/xdg/awesome/?/init.lua",
    "/usr/share/awesome/lib/?.lua",
    "/usr/share/awesome/lib/?/init.lua"
  ],
  "runtime.pathStrict": true
}
