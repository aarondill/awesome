{
  "$schema": "https://raw.githubusercontent.com/LuaLS/vscode-lua/master/setting/schema.json",
  "diagnostics.unusedLocalExclude": ["_*"],
  "workspace": {
    "ignoreDir": [".vscode"],
    "library": ["/usr/share/awesome/lib/", "./deps"]
  },
  "runtime": {
    "version": "Lua 5.3",
    "path": [
      "./deps/.lua/?.lua",
      "./deps/.lua/?/init.lua",
      "./?.lua",
      "./?/init.lua",
      "/usr/share/awesome/lib/?.lua",
      "/usr/share/awesome/lib/?/init.lua"
    ],
    "pathStrict": true
  }
}
